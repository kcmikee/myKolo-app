{
  "expo": {
    "name": "myKolo-app",
    "slug": "myKolo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "splash":{
        "image": "./assets/splash2.png",
        "resizeMode": "cover"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "splash":{
        "image": "./assets/splash2.png",
        "resizeMode": "cover"
      }
    }
  }
}
